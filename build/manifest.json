{
    "name": "septiwatch",
    "version": "0.0.11",
    "manifest_version": 2,
    "description": "Septicycle watch and checkpoint countdown.",
    "icons": {
        "16": "images/16by16.png",
        "19": "images/19by19.png",
        "38": "images/38by38.png",
        "128": "images/128by128.png"
    },
    "default_locale": "en",
    "content_scripts": [
        {
            "matches": [
                "*://*.ingress.com/intel*"
            ],
            "js": [
                "scripts/septiwatch.js"
            ],
            "css": [
                "styles/main.css"
            ],
            "run_at": "document_end"
        }
    ]
}